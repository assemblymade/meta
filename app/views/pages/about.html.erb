              <a href="<%= user_url(user) %>">
                <div class="mx-auto mb2" style="width:96px">
                  <%= react_component 'Avatar', user: AvatarSerializer.new(user), size: 96 %>
                </div>
                <span class="black bold"><%= user.username %></span>
              </a>

              <a href="https://twitter.com/<%= user.twitter_nickname %>">
                <%= react_component 'Icon', icon: 'twitter' %>
              </a>
            </div>
          <% end %>
        </div>
      </div>
    </div>
  </div>

  <section class="_hero hero-about-bottom"> <!-- .hero namespace is being used by legacy code. refactor -->
    <div class="main">
      <div class="container">
        <h1>
          Let's Get to Know Each&nbsp;Other
        </h1>
        <p>
          You seem like a super cool person. Don't be shy, give us a follow and we'll be friends in no&nbsp;time!
        </p>
      </div>
    </div>
    <div class="footer">
      <div class="container">
        <div class="social-row">
          <ul>
            <li>
              <a href="https://twitter.com/asm">
                <img src="/assets/about/social-icon-twitter.png">
              </a>
            </li>
            <li>
              <a href="https://www.facebook.com/assemblymade">
                <img src="/assets/about/social-icon-facebook.png">
              </a>
            </li>
          </ul>
          <div class="pill-bg"></div>
        </div>
      </div>
    </div>
  </section>

</main>

<%= render 'shared/footer' %>


<% content_for :javascript do %>
<script>
    // -- video modal
    (function($) {

      $('.popup-video').magnificPopup({
          disableOn:700,
          type:'iframe',
          mainClass:'mfp-fade',
          removalDelay:160,
          preloader:!1,
          fixedContentPos:!1
      });

    })(jQuery);

  // -- dashboard
  (function($) {
    var $chart = $('.circle-stats .chart');
    //
    $chart.easyPieChart({
      size: '90',
      lineWidth: '10',
      trackColor: '#e5e7ea',
      barColor: '#33d6a6',
      easing: 'easeOutBounce',
      scaleColor: false,
      scaleLength: false,
      onStep: function(from, to, percent) {
        $(this.el).find('.percent').text(Math.round(percent));
      }
    });
  })(jQuery);

  // -- full slider
  (function($) {
    var $fullSlider = $('.full-slider-about'),
        $fullSliderCarousel = $fullSlider.find('.carousel');
    //
    $fullSliderCarousel.owlCarousel({
      singleItem: true,
      slideSpeed: 300,
      paginationSpeed: 400,
      navigation: false,
      afterInit: afterOWLinit,
      afterUpdate: afterOWLinit
    });
    function afterOWLinit() {
      $('.owl-controls .owl-page span').append('<a class="item-link" href="#"/>');
      var pafinatorsLink = $('.owl-controls .item-link');
      //
      $.each(this.owl.userItems, function (i) {
        $(pafinatorsLink[i])
          .css({
            'background': 'url(' + $(this).find('img').attr('src') + ') center center no-repeat',
            '-webkit-background-size': 'cover',
            '-moz-background-size': 'cover',
            '-o-background-size': 'cover',
            'background-size': 'cover'
          })
          .on('click', function(e) {
            e.preventDefault();
          });
      });
    }
  })(jQuery);

  // -- quote slider
  (function($) {
    var $quoteSlider = $('.quote-slider-about'),
        $quoteSliderCarousel = $quoteSlider.find('.carousel');
    //
    $quoteSliderCarousel.owlCarousel({
      singleItem: true,
      autoPlay: 10000,
      slideSpeed: 300,
      paginationSpeed: 400,
      navigation: false
    });
  })(jQuery);

</script>
<% end %>
