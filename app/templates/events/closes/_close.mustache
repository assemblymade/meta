<div class="timeline-item">
  <div class="media">
    <div class="pull-left">
      <div class="marker gray-2 border-gray-2">
        <span class="icon icon-disc"></span>
      </div>
    </div>

    <div class="media-body">
      <div class="media-heading mb0">
        Closed by

        {{#actor}}
          <a href="{{url}}">{{username}}</a>
        {{/actor}}
      </div>

      <div class="markdown-normalized">{{{body_html}}}</div>
    </div>
  </div>
</div>
