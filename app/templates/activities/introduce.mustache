<div class="media" {{#subject}}id="{{anchor}}" data-readraptor-track="{{readraptor_track_id}}"{{/subject}}>
  <div class="pull-left">
    <div class="marker marker-green">
      <i class="icon icon-user-following" style="font-size: 12px;"></i>
    </div>
  </div>

  <div class="media-body">
    <div class="media-heading omega">
      <div class="activity-body">
        <div class="activity-actions">
          <ul class="list-inline omega text-muted">
            <li>
              {{#actor}}
                <a href="{{url}}" title="@{{username}}">
                  @{{username}}
                </a>
                joined the team
              {{/actor}}
            </li>
            {{#id}}
              <li>
                <div class="js-tips"
                     data-tips-to="{{#actor}}{{username}}{{/actor}}"
                     {{#target}}
                       data-tips-path="{{url}}/tips"
                       data-tips-product-id="{{id}}"
                     {{/target}}>
                </div>
              </li>
            {{/id}}
          </ul>
        </div>
        {{#subject}}{{#bio}}
          <div class=" card omega" style="margin-top: 6px; text-align: left; font-size: 16px; padding: 12px 20px; color: #595959;">
            {{bio}}
          </div>
        {{/bio}}{{/subject}}
      </div>

    </div>
  </div>
</div>
